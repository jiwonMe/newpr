{
  "name": "newpr",
  "version": "0.1.3",
  "description": "AI-powered large PR review tool - understand PRs with 1000+ lines of changes",
  "module": "src/cli/index.ts",
  "type": "module",
  "bin": {
    "newpr": "./src/cli/index.ts"
  },
  "files": [
    "src/",
    "README.md"
  ],
  "scripts": {
    "start": "bun run src/cli/index.ts",
    "test": "bun test",
    "lint": "bunx @biomejs/biome check .",
    "lint:fix": "bunx @biomejs/biome check --write .",
    "format": "bunx @biomejs/biome format --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run typecheck && bun test"
  },
  "keywords": [
    "pr",
    "pull-request",
    "code-review",
    "ai",
    "llm",
    "github",
    "cli",
    "tui",
    "claude",
    "openrouter",
    "bun"
  ],
  "author": "Jiwon Park <jiwon@sionic.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiwonMe/newpr.git"
  },
  "homepage": "https://github.com/jiwonMe/newpr",
  "bugs": {
    "url": "https://github.com/jiwonMe/newpr/issues"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "latest",
    "@types/bun": "latest",
    "@types/react": "19.1.0",
    "@types/react-dom": "^19.2.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/cli": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ink": "6.6.0",
    "ink-spinner": "5.0.0",
    "ink-text-input": "6.0.0",
    "lucide-react": "^0.567.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.22.0",
    "tailwind-merge": "^3.4.1",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7"
  }
}
